<ul class="list {{if altLayout 'list--borderbottom' 'list--noborder'}}">
  {{#each model.teaseList as |item|}}
  <li class="list-item">
    {{story-tease
      altLayout=altLayout
      parentTitle=parentTitle
      isStaff=session.data.isStaff
      adminURL=adminURL
      item=item
      playContext=playContext}}
  </li>
  {{/each}}
</ul>
